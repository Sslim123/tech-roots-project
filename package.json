{
	"name": "starter-kit",
	"version": "0.0.1",
	"description": "Starter kit for full-stack JavaScript projects",
	"private": true,
	"scripts": {
		"recreate-db": "node ./server/database_operations.js",
		"prebuild": "npm run cleanup",
		"build": "npm run build:server && npm run build:client",
		"postbuild": "buildinfo dist/static/build-info.txt",
		"build:client": "webpack --config client/webpack/prod.config.js",
		"build:server": "babel server --copy-files --out-dir ",
		"postbuild:server": "rimraf ./dist/**/README.md",
		"cleanup": "rimraf ./dist/*",
		"dev": "concurrently --kill-others \"npm:dev:*\"",
		"dev:client": "webpack serve --config client/webpack/dev.config.js",
		"dev:server": " cross-env LOG_LEVEL=debug PORT=3100 nodemon --inspect server/server.js",
		"lint": "npm run lint:eslint && npm run lint:prettier -- --check",
		"preserve": "npm run build",
		"serve": "npm start",
		"start": "node dist/server.js",
		"lint:eslint": "eslint .",
		"lint:fix": "npm run lint:eslint -- --fix && npm run lint:prettier -- --write",
		"lint:prettier": "prettier .",
		"test:e2e": "npx cypress run"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/textbook/starter-kit.git"
	},
	"author": "Jonathan Sharpe <mail@jonrshar.pe>",
	"contributors": [],
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/textbook/starter-kit/issues"
	},
	"homepage": "https://github.com/textbook/starter-kit#readme",
	"devDependencies": {
		"@babel/cli": "^7.18.10",
		"@babel/core": "^7.20.2",
		"@babel/eslint-parser": "^7.18.9",
		"@babel/node": "^7.18.10",
		"@babel/plugin-proposal-class-properties": "^7.18.6",
		"@babel/plugin-transform-runtime": "^7.18.10",
		"@babel/preset-env": "^7.20.2",
		"@babel/preset-react": "^7.18.6",
		"@codeyourfuture/eslint-config-standard": "^3.0.3",
		"@faker-js/faker": "^7.6.0",
		"@textbook/build-info": "^0.3.1",
		"babel-loader": "^8.2.5",
		"concurrently": "^7.6.0",
		"cross-env": "^6.0.3",
		"css-loader": "^6.7.1",
		"cypress": "^13.15.1",
		"dotenv-webpack": "^8.1.0",
		"eslint": "^8.27.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-cypress": "^2.12.1",
		"eslint-plugin-jsx-a11y": "^6.6.1",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.31.1",
		"eslint-plugin-react-hooks": "^4.6.0",
		"file-loader": "^6.2.0",
		"html-webpack-plugin": "^5.5.0",
		"html-webpack-tags-plugin": "^3.0.2",
		"http-server": "^14.1.1",
		"jest-environment-jsdom": "^29.0.1",
		"nodemon": "^3.1.7",
		"prettier": "^2.7.1",
		"prop-types": "^15.8.1",
		"react": "^18.2.0",
		"react-bootstrap": "^2.6.0",
		"react-dom": "^18.2.0",
		"react-router-dom": "^6.3.0",
		"rimraf": "^3.0.2",
		"socket.io-client": "^4.5.3",
		"style-loader": "^3.3.1",
		"wait-on": "^8.0.1",
		"webpack": "^5.95.0",
		"webpack-cli": "^4.10.0",
		"webpack-dev-server": "^4.10.1",
		"webpack-merge": "^5.8.0"
	},
	"dependencies": {
		"@babel/runtime": "^7.18.9",
		"bootstrap": "^5.2.3",
		"cmd": "^0.1.0",
		"dotenv": "^16.0.2",
		"env": "^0.0.2",
		"express": "^4.18.1",
		"helmet": "^6.0.0",
		"morgan": "^1.10.0",
		"nanoid": "^3.0.0",
		"pg": "^8.8.0",
		"react-icons": "^4.6.0",
		"react-qr-code": "^2.0.8",
		"redirect": "^0.2.0",
		"socket.io": "^4.5.4",
		"winston": "^3.8.1"
	},
	"engines": {
		"node": "^14 || ^16 || ^v18.11.0",
		"npm": "^6 || ^7 || ^8 || ^8.19.2"
	}
}
